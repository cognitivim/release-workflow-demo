{
  "name": "test-commit",
  "version": "1.4.0",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^9.1.0",
    "@commitlint/config-conventional": "^9.0.1",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "commitizen": "^4.1.2",
    "conventional-changelog-conventionalcommits": "^4.3.1",
    "conventional-commit-types": "^3.0.0",
    "cz-customizable": "^6.2.0",
    "dotenv": "^8.2.0",
    "semantic-release": "^17.1.0",
    "standard-version": "^8.0.0"
  },
  "scripts": {
    "commit:lint": "yarn commitlint --from HEAD~1 --to HEAD --verbose",
    "commit:prompt": "yarn git-cz",
    "commit:prompt-retry": "yarn git-cz --retry",
    "s-release:test": "yarn semantic-release --dry-run --no-ci",
    "s-release:local": "yarn semantic-release --no-ci",
    "s-release:ci": "yarn semantic-release",
    "_prepare-release": "yarn standard-version"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cognitivim/release-workflow-demo.git"
  },
  "private": true
}
